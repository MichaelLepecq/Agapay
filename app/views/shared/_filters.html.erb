<div id="sidebar">
  <div id="filters-close" onclick="toggleSidebar(event, this)"><%= image_tag "close.svg", class: 'icon_close', height: '20', width: '20' %></div>
  <ul>
    <% @categories.each do |category| %>
      <li id="charity_category_names_<%= category.name %>" class="<%= category.name %>" onclick="handleEvent(event, this)">
        <%= image_tag "#{category.name}.svg", class: 'icon_cat', height: '25', width: '25' %>
        <p><%= category.name %></p>
      </li>
    <% end %>
  </ul>
</div>
<%= content_for(:after_js) do %>
  <script>

  </script>
<% end %>
